<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>unifi</Name>
  <Repository>brettm357/unifi</Repository>
  <Registry>https://hub.docker.com/r/brettm357/unifi</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Beta>False</Beta>
  <Support />
  <Overview>UniFi Access Point Controller Software - Stable Candidate Version</Overview>
  <Category>Network:Management Tools:</Category>
  <WebUI>https://[IP]:[PORT:8443]</WebUI>
  <TemplateURL>https://github.com/brettm357/docker-templates/blob/master/unifi.xml</TemplateURL>
  <Icon>https://dl.ubnt.com/press/Company_Logos/U_Logo/WEB/U_Logo_RGB.png</Icon>
  <ExtraParams /> 
  <DateInstalled />
  <Description>UniFi Access Point Controller Software - Stable Candidate Version</Description>
  <Networking> 
  <Mode>bridge</Mode> 
  <Publish>
  <Port>
  <HostPort>8080</HostPort> 
  <ContainerPort>8080</ContainerPort> 
  <Protocol>tcp</Protocol> 
  </Port>
  <Port>
  <HostPort>8081</HostPort> 
  <ContainerPort>8081</ContainerPort> 
  <Protocol>tcp</Protocol> 
  </Port>
  <Port>
  <HostPort>8443</HostPort> 
  <ContainerPort>8443</ContainerPort> 
  <Protocol>tcp</Protocol> 
  </Port>
  <Port>
  <HostPort>8843</HostPort> 
  <ContainerPort>8843</ContainerPort> 
  <Protocol>tcp</Protocol> 
  </Port>
  <Port>
  <HostPort>8880</HostPort> 
  <ContainerPort>8880</ContainerPort>  
  <Protocol>tcp</Protocol> 
  </Port>
  <Port>
  <HostPort>6789</HostPort> 
  <ContainerPort>6789</ContainerPort> 
  <Protocol>tcp</Protocol> 
  </Port>
  </Publish>
  </Networking>
  <Data>
  <Volume>
  <HostDir>/mnt/user/appdata/unifi</HostDir> 
  <ContainerDir>/usr/lib/unifi/data</ContainerDir> 
  <Mode>rw</Mode> 
  </Volume>
  </Data>
  <Environment>
  <Variable>
  <Value>99</Value> 
  <Name>PUID</Name> 
  <Mode /> 
  </Variable>
  <Variable>
  <Value>100</Value> 
  <Name>PGID</Name> 
  <Mode /> 
  </Variable> 
  </Environment>
  <Config Name="Host Port 1" Target="8080" Default="8080" Mode="tcp" Description="Container Port: 8080" Type="Port" Display="always" Required="true" Mask="false">8080</Config> 
  <Config Name="Host Port 2" Target="8081" Default="8081" Mode="tcp" Description="Container Port: 8081" Type="Port" Display="always" Required="true" Mask="false">8081</Config> 
  <Config Name="Host Port 3" Target="8443" Default="8443" Mode="tcp" Description="Container Port: 8443" Type="Port" Display="always" Required="true" Mask="false">8443</Config> 
  <Config Name="Host Port 4" Target="8843" Default="8843" Mode="tcp" Description="Container Port: 8843" Type="Port" Display="always" Required="true" Mask="false">8843</Config> 
  <Config Name="Host Port 5" Target="8880" Default="8880" Mode="tcp" Description="Container Port: 8880" Type="Port" Display="always" Required="true" Mask="false">8880</Config> 
  <Config Name="Key 1" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config> 
  <Config Name="Key 2" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config> 
  <Config Name="Host Port 6" Target="6789" Default="6789" Mode="tcp" Description="Container Port: 6789" Type="Port" Display="always" Required="false" Mask="false">6789</Config> 
  <Config Name="AppData Config Path" Target="/usr/lib/unifi/data" Default="/mnt/user/appdata/unifi" Mode="rw" Description="Container Path: /data" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/unifi</Config> 
</Container>
  
  
  
  
  
  
  
